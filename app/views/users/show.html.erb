<div class="row">
  <div class="small-12 column">
    <h1><%= current_user.username %>'s Jokes</h1>
  </div>
</div>
  <% @jokes.each do |joke| %>
<div class="row">
  <div class="small-12 columns">
    <div class="panel"><%= link_to joke.title, "/jokes/#{joke.id}" %></div>
  </div>
</div>
<% end %>

